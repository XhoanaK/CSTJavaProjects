package project9;

	import javax.swing.*;

public class project9 extends JFrame {

		// frame 
		    static JFrame f; 
		  
		    // label to diaplay text 
		    static JLabel l; 
		  
		    // default constructor 
		    text() 
		    { 
		    } 
		  
		    // main class 
		    public static void main(String[] args) 
		    { 
		        // create a new frame to stor text field and button 
		        f = new JFrame("label"); 
		  
		        // create a label to display text 
		        l = new JLabel("new text "); 
		  
		        // create a panel 
		        JPanel p = new JPanel(); 
		  
		        // add label to panel 
		        p.add(l); 
		  
		        // add panel to frame 
		        f.add(p); 
		  
		        // set the size of frame 
		        f.setSize(300, 300); 
		  
		        f.show(); 
		    } 
		}
